<h1> Update Post </h1>
<%= form_with model: @post do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %>
    </div>
    <div>
        <%= form.label :body %>
        <%= form.text_field :body %>
    </div>
    <div>
        <%= form.submit "Update" %>
    </div>

<% end %>
<%= link_to "Back to all posts", posts_index_path %> 
